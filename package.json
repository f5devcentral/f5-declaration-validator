{
  "name": "f5-declaration-validator",
  "version": "0.1.0",
  "description": "f5 atc declaration validator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' 'tests/**/*.{js,ts,tsx}'",
    "watch": "tsc -watch -p ./",
    "compile": "tsc -p ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:debug": "nodemon --inspect src/app.ts",
    "build-package": "npm run compile && npm pack",
    "publish": "npm run compile && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DumpySquare/f5-declaration-validator.git"
  },
  "author": "DumpySquare",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/DumpySquare/f5-declaration-validator/issues"
  },
  "homepage": "https://github.com/DumpySquare/f5-declaration-validator#readme",
  "dependencies": {
    "axios": "^0.25.0",
    "f5-conx-core": "^0.12.4",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1",
    "vscode-json-languageservice": "^4.2.0"
  },
  "devDependencies": {
    "@types/assert": "^1.5.4",
    "@types/deepmerge": "^2.2.0",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "assert": "^2.0.0",
    "eslint": "^7.23.0",
    "mocha": "^9.2.0",
    "nock": "^13.2.1",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.22.11",
    "typescript": "^4.2.4"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "functions": 80,
    "branches": 80,
    "statements": 80,
    "reporter": [
      "html",
      "text"
    ],
    "cache": false
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "mocha": {
    "timeout": 40000
  }
}
